{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "function-paren-newline": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "import/no-dynamic-require": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/href-no-hash": 0,
    "import/prefer-default-export": 0,
    "no-use-before-define": 0,
    "no-debugger": 0
  },
  "extends": ["airbnb-base"]
}